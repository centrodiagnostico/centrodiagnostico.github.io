{
  "server": {
    "request": "server/request.asp"
  },
  "sources": {
    "#": "#estudios",
    "#estudios": "estudios.xml",
    "#mostrar_estudio": "estudios.xml",
    "#ws": {
      "xover.socket.connect": [
        "ws://localhost:8003",
        {
          "message": "#messages",
          "hotreload": "event:hotreload",
          "databaseChange": "event:databaseChange"
        }
      ]
    }
  },
  "start": ["#ws"],
  "settings": {
    "#shell": {
      "hash": ""
    },
    "#estudios": {
      "hash": "",
      "stylesheets": [
        {
          "href": "estudios.xslt",
          "target": "main",
          "action": "replace"
        },
        {
          "href": "estudios_buscador.xslt",
          "target": "#estudios_buscador"
        },
        {
          "href": "estudios_dropdown.xslt",
          "target": "#estudios_dropdown"
        }
      ]
    },
    "#mostrar_estudio": {
      "hash": "",
      "stylesheets": [
        {
          "href": "mostrar_estudio.xslt",
          "target": "main",
          "action": "replace"
        }
      ]
    }
  }
}